generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  // url      = "postgresql://shams:shams1289@localhost:5432/blabla_note_db"
}

model Note {
  id        String   @id @default(uuid())
  text      String
  createdAt DateTime @default(now())
}
